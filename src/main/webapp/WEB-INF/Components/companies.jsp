<script type="text/javascript">

    $("#exp-slider-range").slider({
        range: true,
        min: 0,
        max: 10,
        //values: [0, 10],
        slide: function (event, ui) {
            var min = ui.values[0],
                max = ui.values[1];
            $('#' + this.id).prev().val(min + " year - " + max + " year");
        }
    });

    $("#salary-slider-range").slider({
        range: true,
        min: 10,
        max: 100,
        //values: [10, 1000],
        slide: function (event, ui) {
            var min = ui.values[0],
                max = ui.values[1];
            $('#' + this.id).prev().val(min + "K - " + max + "K");
        }
    });

</script>
